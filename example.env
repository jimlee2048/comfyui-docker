INIT_NODE=true
UPDATE_NODE=false
INIT_MODEL=true
BOOT_CONFIG_INCLUDE=
BOOT_CONFIG_EXCLUDE=(?:example|debug)
COMFYUI_EXTRA_ARGS="--preview-method=auto --disable-auto-launch"

# external api tokens, for downloading access restricted models
# optional but recommended
# https://huggingface.co/settings/tokens
HF_API_TOKEN=
# https://education.civitai.com/civitais-guide-to-downloading-via-api/#how-do-i-download-via-the-api
CIVITAI_API_TOKEN=
# boot process settings
CN_NETWORK=false
# Override Hugging Face endpoint (default: https://huggingface.co, or https://hf-mirror.com if CN_NETWORK=true)
# HF_ENDPOINT=
# Override Civitai endpoint (default: https://civitai.com, or https://civitai.work if CN_NETWORK=true)
# CIVITAI_ENDPOINT=
